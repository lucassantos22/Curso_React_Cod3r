         </>\n        )\n    }\n} \n\nexport default connect('', mapDispatchToProps)(FormComponent);","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/storeConfig.js",[],"/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/reducers/tasks.js",["83"],"const INITIAL_STATE = {\n    tasks: []\n}\n\nexport default (state=INITIAL_STATE, action)=> {\n    switch (action.type) {\n        case 'UPDATE_TASK':\n            return {\n                ...state, tasks: action.payload\n            }\n        default:\n            return state;\n    }\n}","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/actions/tasks.js",[],"/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/reducers/whichTableIsVisible.js",["84"],"const initialState = {\n    whichTableIsVisible: 1\n}\n\nexport default (state = initialState, action)=>{\n    switch (action.type) {\n        case 'VISION_CHANGED':\n            return {\n                ...state, whichTableIsVisible: action.payload\n            }\n        default:\n            return state;\n    }\n}","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/actions/whichTableIsVisible.js",[],"/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/components/templates/alert/Alert.js",["85"],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","severity":1,"message":"91","line":6,"column":1,"nodeType":"92","endLine":16,"endColumn":2},{"ruleId":"90","severity":1,"message":"91","line":8,"column":1,"nodeType":"92","endLine":14,"endColumn":2},{"ruleId":"90","severity":1,"message":"91","line":4,"column":1,"nodeType":"92","endLine":20,"endColumn":2},{"ruleId":"93","severity":1,"message":"94","line":4,"column":8,"nodeType":"95","messageId":"96","endLine":4,"endColumn":13},{"ruleId":"97","severity":1,"message":"98","line":37,"column":5,"nodeType":"99","messageId":"100","endLine":39,"endColumn":6},{"ruleId":"101","severity":1,"message":"102","lin