t className=\"text-muted\">\n                Campo obrigatório\n                </Form.Text>\n            </Form.Group>\n        \n            <Button variant=\"primary\" type=\"submit\" onClick={e => this.sendTask(e)}>\n                Adicionar\n            </Button>\n            </Form>\n            </>\n        )\n    }\n} \n\nexport default connect(mapStateToProps, mapDispatchToProps)(FormComponent);","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/components/templates/Alert.js",["95"],"import {useState} from 'react';\nimport {Alert} from 'react-bootstrap';\n\n\nexport default props => {\n    const [show, setShow] = useState(true);\n\n    return(\n        <Alert style={{position: 'absolute', marginLeft: '38em'}} key={'idx'} variant='success' dismissible show={show} onClose={() => setShow(false)}>\n            Tarefa adicionada com sucesso!\n        </Alert>\n    )\n}","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/serviceWorker.js",[],{"ruleId":"96","severity":1,"message":"97","line":7,"column":13,"nodeType":"98","messageId":"99","endLine":7,"endColumn":26},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":8,"column":1,"nodeType":"106","endLine":14,"endColumn":2},{"ruleId":"104","severity":1,"message":"105","line":4,"column":1,"nodeType":"106","endLine":20,"endColumn":2},{"ruleId":"96","severity":1,"message":"107","line":4,"column":8,"nodeType":"98","messageId":"99","endLine":4,"endColumn":13},{"ruleId":"108","severity":1,"message":"109","line":37,"column":5,"nodeType":"110","messageId":"111","endLine":39,"endColumn":6},{"ruleId":"112","severity":1,"message":"113","line":52,"column":40,"nodeType":"114","messageId":"115","endLine":52,"endColumn":42},{"ruleId":"104","severity":1,"message":"105","line":5,"column":1,"nodeType":"106","endLine":17,"endColumn":2},{"ruleId":"104","severity":1,"message":"105","line":5,"column":1,"nodeType":"106","endLine":14,"endColu