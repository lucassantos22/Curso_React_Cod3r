            </tr>\n                </thead>\n                <tbody>\n                    {this.props.users.map(user=>(\n                        <tr>\n                        <td>{user.first_name}</td>\n                        <td>{user.last_name}</td>\n                        <td>{user.birth_date}</td>\n                        <td>{user.charge}</td>\n                        <td><Button variant=\"outline-danger\" onClick={()=>this.deleteUser(user.id)}>Apagar</Button></td>\n                        </tr>\n                    ))}\n                </tbody>\n            </Table>\n        </>\n        )\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(TableComponent)","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/components/templates/form/Form.js",["79"],"/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/storeConfig.js",[],"/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/reducers/isTableVisible.js",["80"],"const initialState = {\n    isTableVisible: true\n}\n\nexport default (state = initialState, action)=>{\n    switch (action.type) {\n        case 'VISION_CHANGED':\n            return {\n                ...state, isTableVisible: action.payload\n            }\n        default:\n            return state;\n    }\n}","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/actions/isTableVisible.js",[],"/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/reducers/users.js",["81"],"const INITIAL_STATE = {\n    users: []\n}\n\nexport default (state=INITIAL_STATE, action)=> {\n    switch (action.type) {\n        case 'UPDATE_USERS':\n            return {\n                ...state, users: action.payload\n            }\n        default:\n            return state;\n    }\n}","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/actions/users.js",[],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","l