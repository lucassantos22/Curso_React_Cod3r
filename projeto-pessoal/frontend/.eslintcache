) {\n        case 'VISION_CHANGED':\n            return {\n                ...state, whichTableIsVisible: action.payload\n            }\n        default:\n            return state;\n    }\n}","/home/lucas/TI/Curso-React-Cod3r/projeto-pessoal/frontend/src/store/actions/whichTableIsVisible.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":6,"column":1,"nodeType":"86","endLine":16,"endColumn":2},{"ruleId":"84","severity":1,"message":"85","line":8,"column":1,"nodeType":"86","endLine":14,"endColumn":2},{"ruleId":"84","severity":1,"message":"85","line":4,"column":1,"nodeType":"86","endLine":20,"endColumn":2},{"ruleId":"87","severity":1,"message":"88","line":36,"column":5,"nodeType":"89","messageId":"90","endLine":38,"endColumn":6},{"ruleId":"91","severity":1,"message":"92","line":50,"column":72,"nodeType":"93","messageId":"94","endLine":50,"endColumn":74},{"ruleId":"84","severity":1,"message":"85","line":5,"column":1,"nodeType":"86","endLine":17,"endColumn":2},{"ruleId":"84","severity":1,"message":"85","line":6,"column":1,"nodeType":"86","endLine":42,"endColumn":2},{"ruleId":"91","severity":1,"message":"95","line":16,"column":62,"nodeType":"93","messageId":"94","endLine":16,"endColumn":64},{"ruleId":"87","severity":1,"message":"88","line":26,"column":5,"nodeType":"89","messageId":"90","endLine":28,"endColumn":6},{"ruleId":"84","severity":1,"message":"85","line":5,"column":1,"nodeType":"86","endLine":14,"endColumn":2},{"ruleId":"84","severity":1,"message":"85","line":5,"column":1,"nodeType":"86","endLine":14,"endColumn":2},"no-native-reassign",["96"],"no-negated-in-lhs",["97"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","no-useless-constructor","Useless constructor.","MethodDefinition","noUselessConstructor","eqeqeq","Expected '!==' and instead saw '!='.","BinaryExpression","unexpected","Expected '===' and i