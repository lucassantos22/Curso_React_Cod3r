ar = number % 2 == 0 ? 'Par' : 'Impar';\n        setParImpar(parImpar);\n    }, [number]);\n\n    return (\n        <div className=\"UseEffect\">\n            <PageTitle\n                title=\"Hook UseEffect\"\n                subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n            />\n            <SectionTitle title='Exercício #01'/>\n            <div className=\"center\">\n                <div>\n                    <span className=\"text\">Fatorial: </span>\n                    <span className=\"text red\">{fatorial == -1 ? 'Não existe' : fatorial}</span>\n                </div>\n                <input type=\"number\" value={number} onChange={e => setNumber(e.target.value)} className=\"input\"/>\n            </div>\n            <SectionTitle title='Exercício #02'/>\n            <div className=\"center\">\n                <div>\n                    <span className=\"text\">O número é: </span>\n                    <span className=\"text red\">{parImpar}</span>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default UseEffect\n","/home/lucas/TI/Curso-React-Cod3r/hooks/src/views/examples/UseContext.jsx",[],"/home/lucas/TI/Curso-React-Cod3r/hooks/src/views/examples/UseReducer.jsx",[],"/home/lucas/TI/Curso-React-Cod3r/hooks/src/views/examples/UseCustom.jsx",[],"/home/lucas/TI/Curso-React-Cod3r/hooks/src/components/layout/PageTitle.jsx",[],"/home/lucas/TI/Curso-React-Cod3r/hooks/src/components/layout/SectionTitle.jsx",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":7,"column":10,"nodeType":"79","messageId":"80","endLine":7,"endColumn":12},{"ruleId":"77","severity":1,"message":"78","line":22,"column":37,"nodeType":"79","messageId":"80","endLine":22,"endColumn":39},{"ruleId":"77","severity":1,"message":"78","line":36,"column":58,"nodeType":"79","messageId":"80","endLine":36,"endColumn":60},"no-native-reassign",["81"],"no-negated-in-lhs